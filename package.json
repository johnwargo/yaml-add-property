  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnwargo/yaml-add-property"
  },
  "type": "module",
  "main": "yaml-add-property.js",
  "bin": {
    "yaml-add-prop": "yaml-add-property.js"
  },
